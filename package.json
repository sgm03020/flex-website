    "build:modern": "nuxt-ts build --modern=server",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "start:modern": "nuxt-ts start --modern=server",
    "start:static": "http-server ./dist",
    "generate": "nuxt-ts generate",
    "generate:mirror": "cross-env MIRROR=true nuxt-ts generate",
    "generate:express": "cross-env USE_EXPRESS=true nuxt-ts generate",
    "generate:modern": "nuxt-ts generate --modern",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style"
