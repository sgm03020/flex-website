  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "dev0": "cross-env API_URL='http://localhost:3000' nuxt-ts",
    "dev1": "cross-env API_DEBUG=true EVENTS_DEBUG=true API_URL='http://localhost:3000' nuxt-ts",
    "dev2": "cross-env USE_EXPRESS=true API_URL='http://192.168.0.2:3000' nuxt-ts",
    "dev1-1": "cross-env API_URL='http://192.168.0.2:3000' nuxt-ts",
    "dev1-2": "cross-env API_URL='http://192.168.0.2:7071' nuxt-ts",
    "dev2-1": "cross-env API_DEBUG=true API_URL='https://do-it-muscle.tk:443' nuxt-ts",
    "dev2-2": "cross-env API_DEBUG=true API_URL='https://gekko-api:443' nuxt-ts",
    "dev3": "cross-env API_DEBUG=true API_URL='https://sls-jape-dev-flex-service.azurewebsites.net:443' nuxt-ts",
    "dev4": "cross-env API_DEBUG=true API_URL='http://192.168.0.2:7071' nuxt-ts",
    "dev5": "cross-env API_URL='http://192.168.0.2:5000' nuxt-ts",
    "build:modern1": "cross-env API_DEBUG=true API_URL='https://do-it-muscle.tk:443' nuxt-ts build --modern=client",
